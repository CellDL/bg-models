@prefix : <http://celldl.org/templates/vascular#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix bg: <http://celldl.org/ontologies/bond-graph#> .
@prefix tpl: <http://celldl.org/ontologies/model-template#> .

:segment-template
    a tpl:Template ;
    rdfs:label "Vascular segment template" ;
    bg:model <https://models.physiomeproject.org/exposure/segment-model> ;
    tpl:port [
        a tpl:Port ;
        tpl:id :segment-template-node-1 ;
        rdfs:label "Input pressure"
    ], [
        a tpl:Port ;
        tpl:id :segment-template-node-2 ;
        rdfs:label "Output pressure"
    ] ;
    tpl:parameter [
        a tpl:Parameter ;
        tpl:id :segment-parameter-1 ;
        rdfs:label "Parameter description"
        # units/min/max/default
    ] ;
    tpl:state [
        a tpl:State ;
        tpl:id :segment-state-1 ;
        rdfs:label "State description"
        # units/min/max/default
    ] .
