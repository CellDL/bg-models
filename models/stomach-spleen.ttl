@prefix : <#> .
@prefix bg: <http://celldl.org/ontologies/bond-graph#> .
@prefix cdt: <https://w3id.org/cdt/> .
@prefix lib: <http://celldl.org/templates/vascular#> .
@prefix tpl: <http://celldl.org/ontologies/model-template#> .

#                            -- u_GastricC --
#                          /                  \
#   -- u_Aorta -- u_CeliacA                    u_PortalV --
#                          \                  /
#                            -- u_SplenicC --

## This is about using BG model templates to realise a BG model-template

:stomach-spleen
    a bg:Model ;
    bg:component [
        tpl:template lib:segment-template ;
        tpl:connection [
            # nodes have to be known to template
            tpl:node lib:segment-model:bp_1 ;
            bg:node :u_Aorta
        ], [
            tpl:node lib:segment-model:bp_2 ;
            bg:node :u_CeliacA
        ] ;
        bg:parameter [
            tpl:node :segment-model:resistance ;
            bg:name lib:resistance ;
            bg:value "100 kPa.s/L"^^cdt:ucum
        ]
    ], [
        tpl:template lib:segment-template ;
        tpl:connection [
            tpl:node lib:segment-model:bp_1 ;
            bg:node :u_CeliacA
        ], [
            tpl:node lib:segment-model:bp_2 ;
            bg:node :u_GastricC
        ] ;
        bg:parameter [
            tpl:node :segment-model:resistance ;
            bg:name lib:resistance ;
            bg:value "100 kPa.s/L"^^cdt:ucum
        ]
    ], [
        tpl:template lib:segment-template ;
        tpl:connection [
            tpl:node lib:segment-model:bp_1 ;
            bg:node :u_CeliacA
        ], [
            tpl:node lib:segment-model:bp_2 ;
            bg:node :u_SplenicC
        ] ;
        bg:parameter [
            tpl:node :segment-model:resistance ;
            bg:name lib:resistance ;
            bg:value "100 kPa.s/L"^^cdt:ucum
        ]
    ], [
        tpl:template lib:segment-template ;
        tpl:connection [
            tpl:node lib:segment-model:bp_1 ;
            bg:node :u_GastricC
        ], [
            tpl:node lib:segment-model:bp_2 ;
            bg:node :u_PortalV
        ] ;
        bg:parameter [
            tpl:node :segment-model:resistance ;
            bg:name lib:resistance ;
            bg:value "100 kPa.s/L"^^cdt:ucum
        ]
    ], [
        tpl:template lib:segment-template ;
        tpl:connection [
            tpl:node lib:segment-model:bp_1 ;
            bg:node :u_SplenicC
        ], [
            tpl:node lib:segment-model:bp_2 ;
            bg:node :u_PortalV
        ] ;
        bg:parameter [
            tpl:node :segment-model:resistance ;
            bg:name lib:resistance ;
            bg:value "100 kPa.s/L"^^cdt:ucum
        ]
    ] .

:u_Aorta
    bg:value "16 kPa"^^cdt:ucum .

:u_PortalV
    bg:value "5 kPa"^^cdt:ucum .

:u_CeliacA
    bg:parameter [
        bg:name lib:elastance ;
        bg:value "400 kPa/L"^^cdt:ucum
    ], [
        bg:name lib:fixed-volume ;
        bg:value "0.06 L"^^cdt:ucum
    ] ;
    bg:state [
        bg:name lib:volume ;
        bg:value "0.1 L"^^cdt:ucum
    ] .

:u_GastricC
    bg:parameter [
        bg:name lib:elastance ;
        bg:value "400 kPa/L"^^cdt:ucum
    ], [
        bg:name lib:fixed-volume ;
        bg:value "0.06 L"^^cdt:ucum
    ] ;
    bg:state [
        bg:name lib:volume ;
        bg:value "0.1 L"^^cdt:ucum
    ] .

:u_SplenicC
    bg:parameter [
        bg:name lib:elastance ;
        bg:value "400 kPa/L"^^cdt:ucum
    ], [
        bg:name lib:fixed-volume ;
        bg:value "0.06 L"^^cdt:ucum
    ] ;
    bg:state [
        bg:name lib:volume ;
        bg:value "0.1 L"^^cdt:ucum
    ] .
